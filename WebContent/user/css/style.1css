	display: -moz-inline-stack;
	display: inline-block;
	vertical-align: middle;
	zoom: 1;
	-webkit-border-radius: 100px;
	-moz-border-radius: 100px;
	-ms-border-radius: 100px;
	-o-border-radius: 100px;
	border-radius: 100px;
}

#flat-forms-pack .container .container-small .well-big .step {
	color:#23B86C;
}

#flat-forms-pack .container .container-small .well-big .separator {
	border: solid #edeff0;
	border-width: 1px 0 0;
	clear: both;
	height: 0;
	margin: 0px 0 16px -30px;
	width: 110.5%;
}

#flat-forms-pack .container .container-small .well-big .field .visa {
    height: 30px;
    position: absolute;
    margin-top: -4px;
    padding-left: 6px;
}

#flat-forms-pack .container .container-small .well-big .mastercard {
	height: 50px;
    position: absolute;
	margin-top: -14px;
	padding-left: 50px;
}

#flat-forms-pack .container .container-small .well-big .amex {
	height: 30px;
    position: absolute;
    margin-top: -4px;
	padding-left: 102px;
}

#flat-forms-pack .container .container-small .well-big .paypal {
	height: 52px;
	position: absolute;
	margin-top: -15px;
	padding-left: 6px;
}

#flat-forms-pack .container .container-small .well-big .bitcoin {
	height: 83px;
	position: absolute;
	margin-top: -30px;
	padding-left: 0px;
}

#flat-forms-pack .container .container-small .well-big .field .terms {
	font-size: 16px;
	display: inline;
}

#flat-forms-pack .container .container-small .well-big .field span {
	font-size: 13px;
}


