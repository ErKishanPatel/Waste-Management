/* ================= */
/* Responsive Design */
/* ================= */

/*---------------------*/
/*----- Desktops ------*/
/*---------------------*/

/* 1024 x 600 [ 10" & 12" Netbook ] */
@media only screen and (min-width: 1024px) and (max-width: 1279px) {
	.col-sm-12 {
		width: 97.5%;
	}

	.well-big {
		padding-bottom: 110px;
	}
}

/* 1280 x 800 [ 13" Netbook ] */
@media only screen and (min-width: 1280px) and (max-width: 1365px) {
	.col-lg-6 {
		width: 48.8%;
	}
}

/* 1366 x 768 [ 15" Netbook ] */
@media only screen and (min-width: 1366px) and (max-width: 1439px) {
	.col-lg-6 {
		width: 48.7%;
	}
}

/* 1440 x 900 [ 19" Netbook ] */
@media only screen and (min-width: 1440px) and (max-width: 1679px) {
	.col-lg-6 {
		width: 48.7%;
	}
}

/* 1680 x 1050 [ 22" Netbook ] */
@media only screen and (min-width: 1680px) and (max-width: 1919px) {
	.col-lg-6 {
		width: 48.7%;
	}
}

/* 1920 x 1080 [ 23" Netbook ] */
@media only screen and (min-width: 1920px) {
	.col-lg-6 {
		width: 48.8%;
	}
}


/*---------------------*/
/*---- Television -----*/
/*---------------------*/

/* 640 x 480 [ 480p Television ] */
@media only all and (min-width: 640px) and (max-width: 1279px) {
	.col-sm-12 {
		padding-right: 28px;
	}

	.well-big {
		padding-bottom: 16px;
	}
}

/*----------------------*/
/*------ Tablets -------*/
/*----------------------*/

/* 768 x 1366 [ Tablet - iPad Mini & Dell, HP, Asus, Lenovo, Acer... ] */
@media only screen and (min-width: 768px) and (max-width: 1023px) {
	.well-big {
		padding-bottom: 110px;
	}
}

/* 600 x 1024 [ Tablet - Acer, Lenovo, Samsung, Sony, Nexus, Toshiba, Motorola... ] */
@media only screen and (min-width: 600px) and (max-width: 767px) {
	.col-sm-12 {
		padding-right: 27px;
	}

	.well-big {
		padding-bottom: 16px;
	}
}


/*----------------------*/
/*---- Smartphones -----*/
/*----------------------*/

/* 240 x 320 [ Motorola RAZR V8 ] */
@media only screen and (min-width: 240px) and (max-width: 319px) {
	#language-bar {
		margin-right: 38px;
	}

	#flat-forms-pack .container .container-small .heading {
		max-width: 220px;
	}

	.square-gradient-bar {
		margin-top: 60px;
		width: 43%;
	}

	.well-big {
		width: 43%;
		padding-bottom: 16px;
	}

	#flat-forms-pack .container .container-small .well-big .separator {
		width: 128.5%;
	}

	.col-sm-12 {
		padding-right: 27px;
	}

	#flat-forms-pack .well-big form .googleplus-button p, #flat-forms-pack .well-big form .facebook-button p {
		margin-left: 39px;
	}

	#flat-forms-pack .footnote {
		width: 44%;
	}
}

/* 320 x 568 [ iPhone 3,4,5, Blackberry, Samsung Galaxy SII ] */
@media only screen and (min-width: 320px) and (max-width: 359px) {
	#language-bar {
		margin-right: 82px;
	} 

	#flat-forms-pack .container .container-small .heading {
		width: 62%;
	}

	.square-gradient-bar {
		margin-top: 36px;
		width: 60%;
	}

	.well-big {
		width: 60%;
		padding-bottom: 16px;
	}

	.col-sm-12 {
		padding-right: 27px;
	}

	#flat-forms-pack .container .container-small .well-big .separator {
		width: 118.5%;
	}

	#flat-forms-pack .footnote {
		width: 60%;
	}
}

/* 360 x 640 [ Samsung Galaxy S III ] */
@media only screen and (min-width: 360px) and (max-width: 599px) {
	#language-bar {
		margin-right: 104px;
	} 

	#flat-forms-pack .container .container-small .heading {
		width: 68%;
	}

	.square-gradient-bar {
		margin-top: 36px;
		width: 66%;
	}

	.well-big {
		width: 66%;
		padding-bottom: 16px;
	}

	.col-sm-12 {
		padding-right: 27px;
	}

	#flat-forms-pack .container .container-small .well-big .separator {
		width: 116.5%;
	}

	#flat-forms-pack .footnote {
		width: 66%;
	}
}